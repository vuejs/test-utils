{
  "name": "@vue/test-utils",
  "version": "2.0.0-rc.6",
  "license": "MIT",
  "main": "dist/vue-test-utils.cjs.js",
  "unpkg": "dist/vue-test-utils.browser.js",
  "types": "dist/index.d.ts",
  "module": "dist/vue-test-utils.esm-bundler.js",
  "files": [
    "dist",
    "README.md",
    "dist/index.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/types": "^7.14.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.3",
    "@types/pretty": "^2.0.0",
    "@vue/compiler-dom": "^3.0.11",
    "@vue/compiler-sfc": "3.0.11",
    "babel-jest": "^26.6.3",
    "babel-preset-jest": "^26.6.2",
    "husky": "^6.0.0",
    "jest": "25.5.4",
    "jsdom": "^16.5.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.48.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "25.3.1",
    "typescript": "^4.2.4",
    "vitepress": "^0.13.2",
    "vue": "3.0.11",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-jest": "^5.0.0-alpha.9",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.1"
  },
  "author": {
    "name": "Lachlan Miller",
    "email": "lachlan.miller.1990@outlook.com"
  },
  "scripts": {
    "test": "yarn jest --runInBand tests",
    "test:build": "yarn jest --runInBand tests -use-build",
    "tsd": "tsc -p test-dts/tsconfig.tsd.json",
    "build": "yarn rollup -c rollup.config.js",
    "lint": "prettier -c --parser typescript \"(src|tests)/**/*.ts?(x)\"",
    "lint:fix": "yarn lint --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --parser=typescript --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "tsd": {
    "directory": "test-dts",
    "compilerOptions": {
      "strict": false,
      "lib": [
        "esnext",
        "dom"
      ]
    }
  }
}
