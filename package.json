{
  "name": "@vue/test-utils",
  "version": "2.0.0-beta.3",
  "license": "MIT",
  "main": "dist/vue-test-utils.cjs.js",
  "browser": "dist/vue-test-utils.browser.js",
  "unpkg": "dist/vue-test-utils.browser.js",
  "types": "dist/index.d.ts",
  "module": "dist/vue-test-utils.esm-bundler.js",
  "files": [
    "dist",
    "README.md",
    "dist/index.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/types": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/estree": "^0.0.42",
    "@types/jest": "^24.9.1",
    "@types/node": "12.12.35",
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "babel-jest": "^25.2.3",
    "babel-preset-jest": "^25.2.1",
    "dom-event-types": "^1.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.0.0",
    "tsd": "0.11.0",
    "typescript": "^3.7.5",
    "vue": "^3.0.0-rc.10",
    "vue-jest": "vuejs/vue-jest#next",
    "vue-router": "^4.0.0-alpha.14",
    "vuex": "^4.0.0-beta.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0-rc.10"
  },
  "author": {
    "name": "Lachlan Miller",
    "email": "lachlan.miller.1990@outlook.com"
  },
  "scripts": {
    "test": "yarn jest --runInBand tests",
    "test:build": "yarn jest --runInBand tests -use-build",
    "tsd": "tsd",
    "build": "yarn rollup -c rollup.config.js",
    "lint": "prettier -c --parser typescript \"(src|tests)/**/*.ts?(x)\"",
    "lint:fix": "yarn lint --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --parser=typescript --write"
    ]
  },
  "tsd": {
    "directory": "test-dts",
    "compilerOptions": {
      "strict": false,
      "lib": [
        "esnext",
        "dom"
      ]
    }
  }
}
